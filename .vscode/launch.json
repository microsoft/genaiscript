{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "cli",
            "program": "${workspaceFolder}/packages/cli/built/gptools.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "preLaunchTask": "npm: compile-cli",
            "args": [
                "run",
                "front-matter",
                "SUPPORT.md",
                "-ae"
            ]
        },
        {
            "name": "Run - sample",
            "type": "extensionHost",
            "debugWebWorkerHost": true,
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode",
                "${workspaceFolder}/packages/sample"
            ],
            "outFiles": [
                "${workspaceFolder}/packages/vscode/built/**"
            ],
            "preLaunchTask": "npm: compile-ext",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "cwd": "${workspaceFolder}/packages/vscode"
